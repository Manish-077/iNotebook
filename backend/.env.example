# MongoDB Atlas Connection String
# Get this from your MongoDB Atlas dashboard
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/inotebook?retryWrites=true&w=majority&appName=inotebook

# JWT Secret Key
# Use a long, random, secure string - you can generate one at: https://www.allkeysgenerator.com/Random/Security-Encryption-Key-Generator.aspx
JWT_SECRET=your_super_secure_jwt_secret_key_here

# Server Port
PORT=5000
